    "ioredis": "^4.28.2",
    "jest": "^27.4.5",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^8.9.0",
    "mongoose": "^6.0.8",
    "morgan": "^1.10.0",
    "nock": "^13.2.4",
