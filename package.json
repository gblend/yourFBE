{
  "name": "yourfeeds-api",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git://github.com/gblend/yourfeedsApi.git"
  },
  "main": "dist/server",
  "scripts": {
    "dev": "nodemon dist/server",
    "build": "rimraf -rf dist && tsc",
    "postbuild": "copyfiles --error .env Procfile tests/**/*.* .github/**/*.* app/public/*.* postman_collection.json dist",
    "start": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "pm2:dev": "NODE_PATH=. pm2 delete dist/ecosystem.config.js && pm2 start dist/ecosystem.config.js --no-daemon",
    "pm2": "NODE_PATH=. pm2 delete dist/ecosystem.config.js && pm2 start dist/ecosystem.config.js",
    "stop": "NODE_PATH=. pm2 stop dist/ecosystem.config.js",
    "test": "NODE_ENV=test jest --coverage dist/tests/* --passWithNoTests",
    "unit": "jest --coverage dist/tests/unit --passWithNoTests",
    "integration": "NODE_ENV=test jest --coverage dist/tests/integration --watchAll"
  },
  "author": "gabrielilo190@gmail.com",
  "license": "ISC",
